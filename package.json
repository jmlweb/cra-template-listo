{
  "name": "cra-template-listo",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Jos√© Manuel Lucas <josemanuel@jmlweb.es>",
  "engines": {
    "node": ">= 10"
  },
  "main": "template.json",
  "description": "CRA Template with: prop-types, airbnb's eslint config, normalize.css, styled components, react-router, react-helmet and SSG of public routes",
  "keywords": [
    "react",
    "create-react-app",
    "cra-template",
    "template",
    "eslint",
    "prop-types",
    "normalize.css",
    "react-router",
    "react-router-dom",
    "react-helmet",
    "SSG",
    "react-snapshot"
  ],
  "files": [
    "template",
    "template.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jmlweb/cra-template-listo",
    "directory": "/"
  },
  "bugs": {
    "url": "https://github.com/jmlweb/cra-template-listo/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest",
    "access": "public"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && react-snapshot",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactSnapshot": {
    "include": [
      "/about"
    ],
    "exclude": [
      "/_/**"
    ]
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "react-scripts": "4.0.1",
    "react-snapshot": "^1.3.0"
  }
}
